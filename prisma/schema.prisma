generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model User {
    id              Int             @id @map("_id")
    wallet          String          @unique
    questionsSolved QuestionsList[]
}

model QuestionsList {
    id          Int    @id @map("_id")
    router      String
    title       String
    description String
    hint        String
    contract    String
    line        String
    typeattack  String
    User        User?  @relation(fields: [userId], references: [id])
    userId      Int?
}
